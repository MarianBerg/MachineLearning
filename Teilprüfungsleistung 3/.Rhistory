library(tidyverse)
learning_rate <- 0.2
learning_function <- function(t, lambda) {
learning_rate * exp(- t * lambda)
}
data <- seq.int(0, 60)
data <- tibble(x = seq.int(0, 60) %>%
mutate(y_1 = learning_function(x, 0.1),
y_2 = learning_function(x, 0.5),
y_3 = learning_function(x, 0.99)
)
)
